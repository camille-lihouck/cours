/**

 */

public class GrayColor{
    //attributs
    public static final GrayColor WHITE = new GrayColor(255);
    public static final GrayColor BLACK = new GrayColor(0);
    private int level;

    //constructeurs
    public GrayColor(int level){
	this.level = level;
    }

    //m√©thodes
    public int getLevel(){
	return this.level;
    }

    public boolean equals(Object o){
	return false;
    }
}
